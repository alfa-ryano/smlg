context Tree {
    constraint constraint_01 {
        check: 
            self.validation_01()
        message:
            "Every element must have a name"
    }
}

operation Tree validation_01(): Boolean {
    var i := 0;
    Node.allInstances().size().println();
    for (node in Node.allInstances()){
        node.id.println(); 
        if (node.name.trim() == ""){
            return false;
        }
    }
    return true;
}
